    <section class="ftco-section ftco-cart">
			<div class="container">
				<div class="row">
    			<div class="col-md-12 ftco-animate">
    				<div class="car-list">
	    				<table class="table">
						    <thead class="thead-primary">
						      <tr class="text-center">
						        <th>&nbsp;</th>
						        <th>&nbsp;</th>
						        <th class="bg-primary heading">Per Hour Rate</th>
						        <th class="bg-dark heading">Per Day Rate</th>
						        <th class="bg-black heading">Leasing</th>
						      </tr>
						    </thead>
						    <tbody>





								<p><%=items[0].brand%></p>
							<%items.forEach((item)=>{%>

						      <tr class="">
						      	<td class="car-image"><div class="img" style="background-image:url(<%=item.image%>);"></div></td>
						        <td class="product-name">
						        	<h3><%=item.manufacturer%> <%=item.brand%></h3>
						        	<p class="mb-0 rated">
						        		<span>rated:</span>
						        		<span class="ion-ios-star"></span>
						        		<span class="ion-ios-star"></span>
						        		<span class="ion-ios-star"></span>
						        		<span class="ion-ios-star"></span>
						        		<span class="ion-ios-star"></span>
						        	</p>
						        </td>
						        
						        <td class="price">
						        	<p class="btn-custom"><a href="#">Rent a car</a></p>
						        	<div class="price-rate">
							        	<h3>
							        		<span class="num"><small class="currency">$</small><%=item.rentperhour%></span>
							        		<span class="per">/per hour</span>
							        	</h3>
							        	<span class="subheading">$<%=item.rentperhour*0.02%>/hour fuel surcharges</span>
						        	</div>
						        </td>
						        
						        <td class="price">
						        	<p class="btn-custom"><a href="#">Rent a car</a></p>
						        	<div class="price-rate">
							        	<h3>
							        		<span class="num"><small class="currency">$</small><%=item.rentperhour * 23 - ((item.rentperhour * 23) * 0.1)%></span>
							        		<span class="per">/per day</span>
							        	</h3>
							        	<span class="subheading">$<%=item.rentperhour * 24 - ((item.rentperhour * 24) * 0.1) * 0.02%>/day fuel surcharges</span>
						        </div>
						        </td>

						        <td class="price">
						        	<p class="btn-custom"><a href="#">Rent a car</a></p>
						        	<div class="price-rate">
							        	<h3>
							        		<span class="num"><small class="currency">$</small><%=(item.rentperhour * 23 * 29) - ((item.rentperhour * 23 * 29) * 0.2)%></span>
							        		<span class="per">/per month</span>
							        	</h3>
							        	<span class="subheading">$<%=((item.rentperhour * 23 * 29) - ((item.rentperhour * 23 * 29) * 0.2)) * 0.02%>/month fuel surcharges</span>
							        </div>
						        </td>
						      </tr><!-- END TR-->


							<%})%>


















						    </tbody>
						  </table>
					  </div>
    			</div>
    		</div>
