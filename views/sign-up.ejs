<script>
    document.getElementById("registration").addEventListener("keypress",()=>{
      document.getElementById("err").innerHTML = "";       
    });
  $( "form" ).on( "submit", function(e) { 
    e.preventDefault();
    $(function () {
      const payload = {
        "firstName": $('#firstName').val(),
        "lastName": $('#lastName').val(),
        "email": $('#email').val(),
        "password": $('#password').val(),
      };
    $.ajax({
      type: "POST",
      url: "/api/v1/sign-up",
      data: payload,
      error: function(XMLHttpRequest, textStatus, errorThrown) {
        document.getElementById("err").innerHTML = XMLHttpRequest.responseJSON.message;
      },
      success: function (response) {
        setTimeout(() => {
            window.location = response.redirectUrl;
        }, 2000);
      }
    });
  });
});
</script>
