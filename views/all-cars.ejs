		<section class="ftco-section bg-light">
    	<div class="container">
    		<div class="row">
				<%if(items.length > 1){%>

				<%for(item of items){%>
					<div class="col-md-4">
						<div class="car-wrap rounded ftco-animate">
							<div class="img rounded d-flex align-items-end" style="background-image: url(<%=item.image%>);">
							</div>
							<div class="text">
								<h2 class="mb-0"><a href=<%="/api/v1/single-car?id="+item.id%>><%=item.brand%></a></h2>
								<div class="d-flex mb-3">
									<span class="cat"><%=item.manufacturer%></span>
									<p class="price ml-auto"><%=item.rentperhour%> <span>/day</span></p>
								</div>
								<p class="d-flex mb-0 d-block"><a href=<%="/api/v1/rental?car_ids="+item.id%> class="btn btn-primary py-2 mr-1">Book now</a> <a href=<%="/api/v1/single-car?id="+item.id%> class="btn btn-secondary py-2 ml-1">Details</a></p>
							</div>
						</div>
					</div>
			<%}%>


				<%}else{%>

								<p style="color:red; font-size:50px; font-family: monospace; margin: auto">Page Not Found...</p>

			<%}%>

    		</div>
    		<div class="row mt-5">
          <div class="col text-center">
            <div class="block-27">
              <ul>
                <li><a href="/api/v1/fetch-cars?page=1">&lt;</a></li>
                <li class="active"><span>1</span></li>
				<%for(let item = 2; item < items.length; item = item + 1){%>
						<%if(item <= Math.ceil(itemCount/12)){%>
							<li><a href=<%=`/api/v1/fetch-cars?page=${item} `%>><%=item%></a></li>												
						<%}%>
				<%}%>
                <!-- <li><a href="/api/v1/fetch-cars?page=2">2</a></li>
                <li><a href="/api/v1/fetch-cars?page=3">3</a></li>
                <li><a href="/api/v1/fetch-cars?page=4">4</a></li>
                <li><a href="/api/v1/fetch-cars?page=5">5</a></li> -->
                <li><a href="/api/v1/fetch-cars?page=6">&gt;</a></li>
              </ul>
            </div>
          </div>
        </div>
    	</div>
